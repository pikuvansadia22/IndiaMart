    <!-- Breadcrumb Section Begin -->
    <section class="breadcrumb-section set-bg" data-setbg="/images/breadcrumb.jpg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="breadcrumb__text">
                        <h2>Order Details</h2>
                        <div class="breadcrumb__option">
                            <a href="/index">Home</a>
                            <span>Order Details</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Breadcrumb Section End -->

    <!-- Shoping Cart Section Begin -->
    <section class="shoping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="shoping__cart__table">
                        <table>
                            <thead>
                                <tr>
                                    <th class="shoping__product">Products</th>
                                    <th>Price</th>
                                    <th>Quantity</th>
                                    <th>Total</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <% var toatal=0;
                                 for(var i=0; i <products.length; i++) { %>
                                <tr>
                                    <td class="shoping__cart__item">
                                        <img src="data:image/<%=products[i].image.contentType%>;base64,<%=products[i].image.data.toString('base64')%>" alt="" height="100" width="100">
                                        <h5><%= products[i].name %></h5>
                                    </td>
                                    <td class="shoping__cart__price">
                                        <%= products[i].price %>
                                    </td>
                                    <td class="shoping__cart__quantity">
                                        <%= products[i].quantity %>
                                    </td>
                                    <td class="shoping__cart__total">
                                        <% toatal= Number(toatal)+ Number(products[i].quantity) * products[i].price %>
                                        $ <%= Number(products[i].quantity) * products[i].price %>
                                    </td>
                                  
                                </tr>
                            <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="row">
               
                <div class="col-lg-6">
                   
                </div>
           
            </div>
        </div>
    </section>
